<manifest>
    <main-class>com.rong.bots.YakFighter.YakFighter</main-class>
    <name>Simple Yak Fighter</name>
    <tag-line>Easy exp</tag-line><!--Max of 50 chars-->
    <description>Kills Yaks at Neitiznot. Any gear.</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>COMBAT</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>COMBATYak</internal-id>
    <!--The rest are optional-->
    <open-source>False</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Combat</tag>
    </tags>
    <resources>
        <resource>path/to/file.png</resource>
    </resources>
</manifest>